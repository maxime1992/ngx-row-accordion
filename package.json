{
  "name": "ngx-row-accordion-demo",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test:demo": "ng test ngx-row-accordion-demo",
    "test:lib": "ng test ngx-row-accordion",
    "test": "yarn run test:demo --watch false && yarn run test:lib --watch false",
    "lint:demo": "ng lint ngx-row-accordion-demo",
    "lint:demo:fix": "yarn run lint:demo --fix",
    "lint:lib": "ng lint ngx-row-accordion",
    "lint:lib:fix": "yarn run lint:lib --fix",
    "lint": "yarn run lint:demo && yarn run lint:lib",
    "lint:fix": "yarn run lint:demo:fix && yarn run lint:lib:fix",
    "build:demo:prod": "ng build --prod",
    "build:lib:dev": "ng build ngx-row-accordion",
    "build:lib:prod": "ng build ngx-row-accordion --prod",
    "build:lib:watch": "yarn run build:lib:dev && onchange 'projects/**/*.{ts,html,scss}' -- yarn run build:lib:dev",
    "prettier:base-files": "prettier \"./{e2e,cypress,src,projects}/**/*.{scss,ts}\"",
    "prettier:fix": "yarn run prettier:base-files --write",
    "prettier:check": "yarn run prettier:base-files --list-different",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
