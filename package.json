    "@angular/router": "8.0.0",
    "core-js": "2.6.5",
    "rxjs": "6.5.2",
    "uuid": "3.3.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.2",
